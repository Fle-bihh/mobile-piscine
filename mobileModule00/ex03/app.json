{
	"expo": {
		"name": "calculator_app",
		"slug": "calculator_app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/42-porto-logo.png",
		"scheme": "calculator_app",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/42-porto-logo.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/42-porto-logo.png"
		},
		"experiments": {
			"typedRoutes": true
		}
	}
}
