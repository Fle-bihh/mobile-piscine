{
  "expo": {
    "name": "medium_weather_app",
    "slug": "medium_weather_app",
    "version": "1.0.0",
    "icon": "./assets/images/42-porto-logo.png",
    "scheme": "medium_weather_app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/42-porto-logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/42-porto-logo.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ]
  }
}
